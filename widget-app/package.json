  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "silent": true,
    "moduleNameMapper": {
      "^_?src[/](.+)": "<rootDir>/src/$1",
      "^components/(.+)": "<rootDir>/src/components/$1",
      "^pages/(.+)": "<rootDir>/src/pages/$1",
      "^dal/(.+)": "<rootDir>/src/dal/$1",
      "^api/(.+)": "<rootDir>/src/api/$1"
    }
