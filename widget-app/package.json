  "scripts": {
    "start": "react-app-rewired start",
    "build-chunks": "react-app-rewired build",
    "build": "webpack --mode development src/index.js --output ./dist/bundle.js",
    "build-move": "webpack --mode development src/index.js --output ./dist/bundle.js&&npm run move",
    "move": "cd ..&&set movepathfrom=/widget-app/dist/bundle.js&&set movepathto=/main-app/build/static/js/&&set movename=widget_bundle.js&&node ./scripts/movefile.js"
